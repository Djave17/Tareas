Proceso Ejercicio_9
	Definir nota1, nota2, nota3, nota4, nota5, suma Como Real;
	Definir promedio Como Real;
	Escribir 'Escriba la nota #1';
	Leer nota1;
	Escribir 'Escriba la nota #2';
	Leer nota2;
	Escribir 'Escriba la nota #3';
	Leer nota3;
	Escribir 'Escriba la nota #4';
	Leer nota4;
	Escribir 'Escriba la nota #5';
	Leer nota5;
	suma <- nota1+nota2+nota3+nota4+nota5;
	promedio <- suma/5;
	Escribir 'Su promedio es: ', promedio;
FinProceso
